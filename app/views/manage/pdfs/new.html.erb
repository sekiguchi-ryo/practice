<h1>pdf_index</h1>
<div>
  <div>
    <%= form_with(model: @pdf, url: manage_pdfs_path, data: { turbo: false }) do |f| %>
      <%= f.label :pdf %>
      <%= f.file_field :pdf %>
      <%= f.submit "Create pdf", class: "text-white bg-blue-700 hover:bg-blue-800 focus:outline-none focus:ring-4 focus:ring-blue-300 font-medium rounded-full text-sm px-5 py-2.5 text-center me-2 mb-2 dark:bg-blue-600 dark:hover:bg-blue-700 dark:focus:ring-blue-800" %>
    <% end %>
  </div>
  <%= render @pdfs %>
</div>